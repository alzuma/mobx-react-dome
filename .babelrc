
{
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "browsers": [
              ">0.25%",
              "not op_mini all"
            ]
          },
          "useBuiltIns": "usage"
        }
      ],
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "plugins": [
      ["@babel/plugin-proposal-decorators", { "legacy": true }],
      ["@babel/plugin-proposal-class-properties", { "loose": true }],
      "@babel/plugin-transform-arrow-functions"
    ]
  }